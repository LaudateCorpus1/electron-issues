      "id": 1268976,
      "avatar_url": "https://avatars1.githubusercontent.com/u/1268976?v=3",
      "gravatar_id": "",
      "url": "https://api.github.com/users/brian-mann",
      "html_url": "https://github.com/brian-mann",
