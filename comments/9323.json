      "starred_url": "https://api.github.com/users/dfbaskin/starred{/owner}{/repo}",
      "subscriptions_url": "https://api.github.com/users/dfbaskin/subscriptions",
      "organizations_url": "https://api.github.com/users/dfbaskin/orgs",
